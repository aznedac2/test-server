<html>
  <head>
    <body>
      <p> id="hello" </p>
    </body>
  </head>
  <script>
    fetch('http://localhosl:3000/users')
      .then(res => res.json())
      .then(data => {
        const users = data.arr;
        let html = '';
        for (let user of users) {
          html += '<p>' + user.name + '<./p>'
        }
        document.getElementById('hello').innerHTML = html;
      })
  </script>
</html>